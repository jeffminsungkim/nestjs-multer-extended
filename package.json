{
  "name": "nestjs-multer-extended",
  "version": "0.0.1",
  "description": "Extended MulterModule for NestJS",
  "author": "Minsung Kim",
  "license": "MIT",
  "url": "https://github.com/jeffminsungkim/nest-multer-extended#readme",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test:integration": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffminsungkim/nest-multer-extended"
  },
  "keywords": [
    "nestjs",
    "nest",
    "multer",
    "file upload",
    "file interceptor",
    "extend",
    "extended",
    "aws",
    "s3"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/platform-express": "^6.10.14"
  },
  "dependencies": {
    "aws-sdk": "^2.608.0",
    "multer-sharp-s3": "^0.1.0",
    "nestjs-config": "^1.4.5",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/testing": "6.10.14",
    "@types/express": "4.17.2",
    "@types/jest": "24.9.0",
    "@types/multer": "^1.3.10",
    "@types/node": "13.1.8",
    "@types/sharp": "^0.24.0",
    "@types/supertest": "2.0.8",
    "husky": "^4.2.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "supertest": "4.0.2",
    "ts-jest": "25.0.0",
    "ts-node": "8.6.2",
    "tsc-watch": "4.1.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.0.0",
    "typescript": "3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
